        <script type="text/javascript">
            var website_id = "1";
            $(function () {
                var nowDate = new Date();
                var str = nowDate.getFullYear() + "-" + (nowDate.getMonth() + 1) + "-" + nowDate.getDate();
                $("#startdate").val(str);
                $("#enddate").val(str);
                $("#begindate").val(getYestoday(new Date()));
                $("#overdate").val(getYestoday(new Date()));

                $(".subnav ul li").each(function () {
                    $(this).removeClass("current");
                });

                var ctime = $("#startdate").val() == $("#enddate").val() ? $("#startdate").val() : $("#startdate").val() + "-" + $("#enddate").val()
                $("#ctime").html(ctime);
            });


            var menuType = [{ id: "001", name: "流量分析" },
            { id: "002", name: "来源分析" },
            { id: "003", name: "受访分析" },
            { id: "004", name: "访客分析"}];

        </script>
